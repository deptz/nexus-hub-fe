{
  "name": "nexus-hub-fe",
  "version": "1.0.0",
  "description": "Nexus Hub Admin - Vue.js + Vite admin dashboard for the Nexus Hub API",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deptz/nexus-hub-fe.git"
  },
  "homepage": "https://github.com/deptz/nexus-hub-fe#readme",
  "bugs": {
    "url": "https://github.com/deptz/nexus-hub-fe/issues"
  },
  "keywords": [
    "vue",
